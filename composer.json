{
  "version": "1.0.0",
  "name": "xditn/laravel-mmodule",
  "description": "laravel多模块启动脚本",
  "type": "library",
  "require": {
    "php": "^8.2",
    "ext-pdo": "*",
    "ext-zip": "*",
    "laravel/sanctum": "^v4.0.8",
    "w7corp/easywechat": "^6.17.5",
    "yansongda/pay": "v3.7.10",
    "maatwebsite/excel": "^3.1.64",
    "nette/php-generator": "^v4.1.8",
    "intervention/image-laravel": "^1.5"
  },
  "require-dev": {
    "laravel/framework": "^12.0"
  },
  "license": "MIT",
  "autoload": {
    "files": [
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "Xditn\\": "src/",
      "modules\\": "src/Base/modules"
    },
    "exclude-from-classmap": [
      "src/Base/modules/*"
    ]
  },
  "authors": [
    {
      "name": "lan",
      "email": "532006864@qq.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Xditn\\Providers\\XditnServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
